<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/transparent"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/woying_20_dip"
        android:background="@color/transparent"
        android:gravity="center_horizontal"
        android:orientation="vertical">

        <TextView
            android:id="@+id/tv_cancle_order"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center_horizontal"
            android:padding="@dimen/woying_14_dip"
            android:text="@string/cancel_order_form"
            android:background="@drawable/round_white"
            android:textColor="@color/font_red"
            android:textSize="16sp" />

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/woying_10_dip"
            android:background="@color/transparent"
            />

        <TextView
            android:id="@+id/tv_cancle"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center_horizontal"
            android:background="@drawable/round_white"
            android:padding="@dimen/woying_14_dip"
            android:text="@string/btn_cancel"
            android:textColor="@color/sign_font_black"
            android:textSize="16sp" />

    </LinearLayout>

</LinearLayout>
